{
  "env": {
    "commonjs": true,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "plugins": ["prettier", "eslint-plugin-react"],
  "extends": [
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "error",
    "quotes": ["single"]
  }
}